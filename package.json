{
  "name": "componente-01-sillumiquinga",
  "version": "0.0.2",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/componente-01-sillumiquinga/componente-01-sillumiquinga.esm.js",
  "exports": {
    ".": {
      "import": "./dist/componente-01-sillumiquinga/componente-01-sillumiquinga.esm.js",
      "require": "./dist/componente-01-sillumiquinga/componente-01-sillumiquinga.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Wilz22/table-component.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.20.0",
    "@types/datatables.net": "^1.10.28",
    "@types/jest": "^29.5.12",
    "@types/jquery": "^3.5.30",
    "@types/node": "^16.18.11",
    "@types/puppeteer": "^5.4.7",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^23.1.0"
  },
  "license": "MIT",
  "dependencies": {
    "@material/data-table": "^14.0.0",
    "datatables.net": "^2.1.3",
    "datatables.net-dt": "^2.1.3",
    "jquery": "^3.7.1"
  }
}
